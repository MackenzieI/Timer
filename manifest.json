{
    "manifest_version": 2,
    "name": "inbrowserTimer",
    "version": "1.0",
  
    "description": "Adds a timer to the browser",
    "icons": {
      "48": "icons/timer-48.png"
    },
    "permissions": ["activeTab"],
  
    "browser_action": {
      "default_icon": "icons/timer-32.png",
      "default_title": "Timer",
      "default_popup": "popup/timer.html"
    },
  
    "browser_specific_settings": {
      "gecko": {
        "id": "inbrowserTimer@example.com"
      }
    }
  }
  